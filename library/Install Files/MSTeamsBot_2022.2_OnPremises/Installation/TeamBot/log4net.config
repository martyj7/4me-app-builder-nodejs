<?xml version="1.0"?>
<log4net debug="true">
  <!-- Define some output appenders -->
  <appender name="trace" type="log4net.Appender.TraceAppender, log4net">
    <layout type="log4net.Layout.PatternLayout,log4net">
      <param name="ConversionPattern" value="%d{ABSOLUTE} %-5p %c{1}:%L - %m%n" />
    </layout>
  </appender>
  <appender name="console" type="log4net.Appender.ConsoleAppender, log4net">
    <layout type="log4net.Layout.PatternLayout,log4net">
      <param name="ConversionPattern" value="%d{ABSOLUTE} %-5p %c{1}:%L - %m%n" />
    </layout>
  </appender>
  <appender name="LogFileAppender" type="log4net.Appender.FileAppender">
    <param name="File" value="Logs\\logBot.txt"/>
    <layout type="log4net.Layout.PatternLayout">
      <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n"/>
    </layout>
    <filter type="log4net.Filter.LoggerMatchFilter">
      <loggerToMatch value="AppCache" />
      <acceptOnMatch value="false" />
    </filter>
  </appender>
  <appender name="rollingFile" type="log4net.Appender.RollingFileAppender,log4net" >
    <param name="File" value="Logs\\logBot.txt" />
    <param name="AppendToFile" value="true" />
    <param name="RollingStyle" value="Date" />
    <param name="DatePattern" value="yyyy.MM.dd" />
    <param name="StaticLogFileName" value="true" />
    <layout type="log4net.Layout.PatternLayout,log4net">
      <param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n" />
    </layout>
    <filter type="log4net.Filter.LoggerMatchFilter">
      <loggerToMatch value="AppCache" />
      <acceptOnMatch value="false" />
    </filter>
  </appender>
  <appender name="AppCacheAppender" type="log4net.Appender.RollingFileAppender,log4net" >
    <param name="File" value="Logs\\ApplicationCacheBot.log" />
    <param name="AppendToFile" value="true" />
    <param name="RollingStyle" value="Date" />
    <param name="DatePattern" value="yyyy.MM.dd" />
    <param name="StaticLogFileName" value="true" />
    <layout type="log4net.Layout.PatternLayout,log4net">
      <param name="ConversionPattern" value="%d [%3t] %m%n" />
    </layout>
  </appender>
  <!--<appender name="HttpAppender" type="log4net.Appender.HttpAppender, log4netHttpAppender" >
    <url value="http://du-dev-akhodus2/SaaSLogListener/LogListener.ashx" />
  </appender>-->
  <appender name="FrsXMLAppender" type="log4net.Appender.RollingFileAppender" >
    <param name="File" value="c:\\Logs\\TeamsBotService.frslog" />
	<param name="PreserveLogFileNameExtension" value="true" />
    <param name="AppendToFile" value="true" />
    <param name="RollingStyle" value="Composite" />
    <param name="DatePattern" value="yyyyMMdd-HHmm" />
    <param name="maxSizeRollbackups" value="0" />
    <param name="maximumFileSize" value="10MB" />
    <param name="CountDirection" value="1" />
    <param name="StaticLogFileName" value="true" />
    <layout type="log4net.Layout.XmlLayoutSchemaLog4j">
    <param name="ConversionPattern" value="%timestamp %d %level%n %logger%n [%property{LogEntryId}]%n [%property{tenant}]%n 
             [%property{LoginId}]%n [%property{CurrentRole}]%n [%property{SessionId}]%n [%property{SubsystemId}]%n 
             [%property{ClientIPAddress}]%n [%property{ServiceName}]%n %m%n %exception%n" />
    </layout>
    <!-- 
    whitelist loggers - FrontRangeSaaS and SaaS.WebUI.Global
    blacklist all others 
    filters are executed in sequence
    <filter type="log4net.Filter.LoggerMatchFilter">
      <loggerToMatch value="FrontRangeSaaS" />
      <acceptOnMatch value="true" />
    </filter>
    <filter type="log4net.Filter.LoggerMatchFilter">
      <loggerToMatch value="SaaS.WebUI.Global" />
      <acceptOnMatch value="true" />
    </filter>
    <filter type="log4net.Filter.DenyAllFilter" />
    -->
  </appender>
  <appender name="FrsXMLAppenderToDiskOnly" type="log4net.Appender.RollingFileAppender,log4net" >
    <param name="File" value="c:\\Logs\\TeamsBotService.frsDiskLog" />
    <param name="AppendToFile" value="false" />
    <param name="RollingStyle" value="Composite" />
    <param name="DatePattern" value="yyyyMMdd" />
    <param name="maxSizeRollbackups" value="-1" />
    <param name="maximumFileSize" value="1MB" />
    <param name="CountDirection" value="1" />
    <param name="StaticLogFileName" value="true" />
    <layout type="log4net.Layout.XMLLayout,log4net">
      <!--<layout type="log4net.Layout.XMLLayoutSchemaLog4j,log4net">-->
      <param name="ConversionPattern" value="%timestamp %d %level%n %logger%n [%property{LogEntryId}]%n [%property{tenant}]%n 
             [%property{LoginId}]%n [%property{CurrentRole}]%n [%property{SessionId}]%n [%property{SubsystemId}]%n 
             [%property{ClientIPAddress}]%n [%property{ServiceName}]%n %m%n %exception%n" />
    </layout>
  </appender>
  <!-- Setup the root category, add the appenders and set the default priority -->
  <root>
    <level value="ALL" />
    <appender-ref ref="FrsXMLAppender" />
    <!--
    <appender-ref ref="FrsXMLAppenderToDiskOnly" />
    <appender-ref ref="LogFileAppender" />
    <appender-ref ref="HttpAppender" />
    -->
  </root>
  <!--
  <logger name="AppCache">
    <level value="ALL" />
    <appender-ref ref="AppCacheAppender" />
  </logger>
  -->
</log4net>